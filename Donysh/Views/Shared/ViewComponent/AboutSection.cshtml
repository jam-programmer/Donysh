@model HomeAboutSection
<section class="section">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-7">
                <div class="right-img-block">
                    <div class="section-title">
                        <h2 class="theme-after">Why Choose us</h2>
                        <div class="col-md-11 col-lg-11 text-center p-0px-lr m-10px-t">
                            <p>@Html.Raw(Model.AboutDescription)</p>
                            <div class="row">
                                <div class="col-md-5 col-lg-5 p-0px-lr text-left m-10px-t p-15px-l"><span>@Model.WorkExperience</span><p class="title">Years of Experience</p></div> 
                                <div class="col-md-5 col-lg-5 p-0px-lr text-left m-10px-t p-15px-l"><span>@Model.CompletedProject</span><p class="title">Completed Project </p></div></div>
                        </div>
                    </div>
                </div>
            </div> <!-- col -->
            <div class="col-md-5 sm-m-30px-t">
                <img src="~/Setting/@Model.AboutImage" title="" alt="">
            </div>
        </div><!-- row -->
    </div>
</section>